pipeline {
  agent any
  stages
    {
    stage('Git checkout') {
      steps {
        git changelog: false, poll: false, url: 'https://github.com/Anji399/canara.git' 
      }
    }
    stage('Package') {
      steps {
        sh 'mvn package'
      }
    }
    stage('Build docker image') {
      steps {
        sh 'docker build -t mvpar/canara:1.0.2 .'  
      }    
    }
   stage('Push docker image') {
      steps {
       withCredentials([string(credentialsId: 'dddd', variable: 'dock')]) {
        sh "docker login -u mvpar -p ${dock}"
       }  
        sh 'docker push mvpar/canara:1.0.2'
      }    
    }
  }
}  
